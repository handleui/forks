{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "tui",
  "tasks": {
    "//#check": {},
    "//#fix": { "cache": false },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", "out/**", ".next/**", "!.next/cache/**"]
    },
    "typecheck": { "dependsOn": ["^typecheck", "^build"] },
    "check-types": { "dependsOn": ["^check-types", "^build"] },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    }
  }
}
