{
  "name": "@forks-sh/store",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun --sourcemap --external drizzle-orm --external @forks-sh/protocol && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap --outDir dist",
    "check-types": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@forks-sh/protocol": "*",
    "drizzle-orm": "^0.38.0"
  },
  "devDependencies": {
    "@forks-sh/typescript-config": "*",
    "@types/bun": "latest",
    "drizzle-kit": "^0.30.0"
  }
}
